mixin menuentryprice(entry)
    if entry.prices
      span.prices
        span.priceblock
          span.pricelabel= entry.prices[0][0]
          span.pricevalue= "kr " + entry.prices[0][1] + ",-"
        span.priceblock
          span.pricelabel= entry.prices[1][0]
          span.pricevalue= "kr " + entry.prices[1][1] + ",-"
        if entry.prices[2]
          span.priceblock
            span.pricelabel= entry.prices[2][0]
            span.pricevalue= "kr " + entry.prices[2][1] + ",-"
    else
      - 
        var pricestr = typeof(entry.price) == "string" ? entry.price : "kr " + entry.price + ",-"
      span.price= pricestr

mixin menuentry(entrygroup, entry)
  p.position-relative.menuentry(class=entrygroup.classes)
    span.wrap(class=(entry.prices ? "multiprices" : "singleprice") + (entry.description ? " withdesc" : ""))
      span.name(class=entry.description ? "d-block" : "")!= entry.name
      if entry.description
        span.description= entry.description
      span.dots
      +menuentryprice(entry)
      if entry.extra
        span.extra!= entry.extra
      
      if entry.allergens
        span.allergens.d-block= "Allergener: " + entry.allergens

